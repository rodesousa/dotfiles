{
  "codeLens.enable": true,
  "elixirLS.mixEnv": "dev",
  "elixirLS.dialyzerEnabled": false,
  "languageserver": {
    "elixirLS": {
      "command": "~/.elixir-ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["rust", "go", "golang", "java", "elixir", "ts", "typescript"],
  "typescript.referencesCodeLens.enable": false
}
